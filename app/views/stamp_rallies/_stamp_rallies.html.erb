<div class="row">
  <div class="col-12">
    <div class="row">
      <% if stamp_rallies.present? %>
        <%= render stamp_rallies %>
      <% elsif !stamp_rallies.present? %>
        <p>ありませぬ</p>
      <% elsif user_signed_in? == false %>
        <p>ログインしてね！</p>
        <%= link_to t('defaults.login'), login_path %>
      <% end %>
    </div>
  </div>
</div>
