<div class="row">
  <div class="col-12">
    <div class="row">
      <% if stamp_rallies.present? %>
        <%= render stamp_rallies %>
        <div class="col-12 d-flex justify-content-center">
          <%= paginate stamp_rallies, theme: 'bootstrap-5', param_name: page_param %>
        </div>
      <% else %>
        <h3 class="text-center"><%= t 'defaults.message.not_present' %></h3>
      <% end %>
    </div>
  </div>
</div>
